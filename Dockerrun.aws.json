{
  "AWSEBDockerrunVersion": "2",
  "containerDefinitions": [
    {
      "name": "rabbitmq",
      "image": "rabbitmq:latest",
      "environment": [
        {
          "name": "RABBITMQ_DEFAULT_USER",
          "value": "admin"
        },
        {
          "name": "RABBITMQ_DEFAULT_PASS",
          "value": "mypass"
        }
      ],
      "essential": true,
      "memory": 128,
      "portMappings": [{
          "hostPort": 5672,
          "containerPort": 5672
      }]
    },
    {
      "name": "web",
      "image": "brightblackbox/pensiondata_web:latest",
      "environment": [
        {
          "name": "DJANGO_SETTINGS_MODULE",
          "value": "settings.prod"
        }
      ],
      "essential": true,
      "memory": 128,
      "portMappings": [{
          "hostPort": 8000,
          "containerPort": 8000
      }],
      "links": [
        "rabbitmq"
      ]
    },
    {
      "name": "celery",
      "image": "brightblackbox/pensiondata_celery:latest",
      "environment": [
        {
          "name": "DJANGO_SETTINGS_MODULE",
          "value": "settings.prod"
        }
      ],
      "essential": true,
      "memory": 128,
      "links": [
        "rabbitmq"
      ]
    },
  ]
}